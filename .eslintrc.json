{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "prettier", "next/core-web-vitals"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "quotes": ["error", "double"],
    "semi": ["error", "always"],
    "prettier/prettier": "error"
  }
}